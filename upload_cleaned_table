CREATE TABLE lol_original_data (
	gameID integer PRIMARY KEY,
	blueWins integer,
	blueWardsPlaced_diff integer,
	blueWardsDestroyed_diff integer,
	blueFirstBlood integer,
	blueKills_diff integer,
	blueDeaths_diff integer,
	blueAssists_diff integer,
	blueEliteMonsters_diff integer,
	blueDragons_diff integer,
	blueHeralds_diff integer,
	blueTowersDestroyed_diff integer,
	blueAvgLevel_diff float8,
	blueTotalExperience integer,
	blueTotalMinionsKilled_diff integer,
	blueTotalJungleMinionsKilled_diff integer,
	blueGoldDiff integer,
	blueExperienceDiff integer,
	blueCSPerMin_diff float8,
	blueGoldPerMin_diff float8,
);