CREATE TABLE lol_original_data (
	gameID integer PRIMARY KEY,
	blueWins integer,
	blueWardsPlaced integer,
	blueWardsDestroyed integer,
	blueFirstBlood integer,
	blueKills integer,
	blueDeaths integer,
	blueAssists integer,
	blueEliteMonsters integer,
	blueDragons integer,
	blueHeralds integer,
	blueTowersDestroyed integer,
	blueTotalGold integer,
	blueAvgLevel float8,
	blueTotalExperience integer,
	blueTotalExperience integer,
	blueTotalMinionsKilled integer,
	blueGoldDiff integer,
	blueExperienceDiff integer,
	blueCSPerMin float8,
	blueGoldPerMin float8,
	redWardsPlaced integer,
	redWardsDestroyed integer,
	redFirstBlood integer,
	redKills integer,
	redDeaths integer,
	redAssists integer,
	redEliteMonsters integer,
	redDragons integer,
	redHeralds integer,
	redTowersDestroyed integer,
	redTotalGold integer,
	redAvgLevel float8,
	redTotalExperience integer,
	redTotalMinionsKilled integer,
	redTotalJungleMinionsKilled integer,
	redGoldDiff integer,
	redExperienceDiff integer,
	redCSPerMin float8,
	redGoldPerMin float8
);